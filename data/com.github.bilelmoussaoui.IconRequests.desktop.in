[Desktop Entry]
Name=Icon Requests
GenericName=Missing Icon Requests
Comment=Report Missing icons to your current theme
Icon=com.github.bilelmoussaoui.IconRequests
Exec=icon-requests
Type=Application
Terminal=false
Categories=GNOME;GTK;
Keywords=Gnome;GTK;Verification;
StartupNotify=true
X-GNOME-Gettext-Domain=IconRequests
StartupWMClass=icon-requests
Actions=AboutDialog;

[Desktop Action AboutDialog]
Exec=icon-requests --about
Name=About Icon Requests
